{
  "version": 1,
  "projects": {
    "data-store": {
      "projectType": "library",
      "root": "libs/data-store",
      "sourceRoot": "libs/data-store/src",
      "prefix": "restaurante",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/data-store"
          ],
          "options": {
            "jestConfig": "libs/data-store/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/data-store/src/**/*.ts",
              "libs/data-store/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "login": {
      "projectType": "application",
      "root": "apps/login",
      "sourceRoot": "apps/login/src",
      "prefix": "restaurante",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:webpack-browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/login",
            "index": "apps/login/src/index.html",
            "main": "apps/login/src/main.ts",
            "polyfills": "apps/login/src/polyfills.ts",
            "tsConfig": "apps/login/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/login/src/favicon.ico",
              "apps/login/src/assets"
            ],
            "styles": [
              "apps/login/src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/login/webpack.config.js"
            },
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/login/src/environments/environment.ts",
                  "with": "apps/login/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/login/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "login:build:production"
            },
            "development": {
              "browserTarget": "login:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201,
            "publicHost": "http://localhost:4201"
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "login:build",
            "extraWebpackConfig": "apps/login/webpack.config.js"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/login/src/**/*.ts",
              "apps/login/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/login"
          ],
          "options": {
            "jestConfig": "apps/login/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "login-e2e": {
      "root": "apps/login-e2e",
      "sourceRoot": "apps/login-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/login-e2e/cypress.json",
            "devServerTarget": "login:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "login:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/login-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "login"
      ]
    },
    "shell": {
