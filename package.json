{
  "name": "restaurante",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "start:shell": "ng serve shell -o --port 4200",
    "start:login": "ng serve login -o --port 4201",
    "start:all": "concurrently \"npm run start:shell\" \"npm run start:login\"",
    "serve": "concurrently \"serve dist/shell -l 4200 -s\" \"serve dist/login -l 4201 -s\"",
    "build": "nx build",
    "test": "nx test --coverage",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
